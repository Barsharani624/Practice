/*
 For a given integer ‘N’, they want to make the Reverse N-Number Triangle.

Example:
Input: ‘N’ = 3

Output: 

1 2 3
1 2
1
*/
#include <iostream>
using namespace std;

int main()
{
     int n ;
   cin >> n;
    
    for(int i =0 ; i < n ; i++)
	{
		for(int j =0 ; j < n ; j++)
		{
		    if (j< n-i)
		    {
		        cout << j+1;
		    }
			
		}
		cout <<  '\n';
	}

    return 0;
}
